{
  "name": "rollup-plugin-uglify",
  "version": "1.0.1",
  "description": "Rollup plugin to minify generated bundle",
  "main": "dist/rollup-plugin-uglify.js",
  "module": "dist/rollup-plugin-uglify.es.js",
