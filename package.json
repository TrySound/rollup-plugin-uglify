  "dependencies": {
    "babel-code-frame": "^6.20.0",
    "uglify-js": "^2.6.1"
  },
  "devDependencies": {
    "buble": "^0.10.6",
    "mocha": "^2.5.3",
    "rollup": "^0.31.1",
    "rollup-plugin-buble": "^0.10.0"
  }
